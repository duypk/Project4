		"LS_SqlDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_SqlDb'"
		},
		"LS_StorageGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_StorageGen2'"
		},
		"LS_Synapse_connectionString": {
